{
  "name": "hyperamp",
  "productName": "Hyperamp",
  "description": "Indescribable music player",
  "version": "0.0.1",
  "author": "Hypermodules (https://github.com/hypermodules)",
  "bugs": {
    "url": "https://github.com/hypermodules/hyperamp/issues"
  },
  "contributors": [
    "Bret Comnes <bcomnes@gmail.com> (http://bret.io)",
    "Nate Goldman <ungoldman@gmail.com> (http://ungoldman.com)"
  ],
  "dependencies": {
    "@bret/truthy": "^1.0.1",
    "cache-component": "^5.0.0-1",
    "choo": "^5.6.1",
    "choo-log": "^6.0.0",
    "classnames": "^2.2.5",
    "csjs-inject": "^1.0.1",
    "electron-config": "^1.0.0",
    "electron-context-menu": "^0.9.0",
    "electron-debug": "^1.0.1",
    "electron-default-menu": "^1.0.1",
    "electron-window-state": "^4.0.2",
    "entypo": "^2.0.0",
    "flush-write-stream": "^1.0.2",
    "folder-walker": "^3.0.0",
    "format-duration": "^1.0.0",
    "global": "^4.3.2",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "music-metadata": "^0.6.1",
    "musicmetadata": "^2.0.4",
    "nanobus": "^4.0.0",
    "nanocomponent": "^4.0.0",
    "nanologger": "^1.0.2",
    "nanotick": "^1.1.6",
    "pump": "^1.0.2",
    "run-parallel": "^1.1.6",
    "through2-filter": "^2.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "dependency-check": "^2.8.0",
    "electron": "^1.6.8",
    "electron-builder": "^17.9.0",
    "electron-rebuild": "^1.5.11",
    "npm-run-all": "^4.0.1",
    "snazzy": "^7.0.0",
    "spectron": "^3.6.4",
    "standard": "^10.0.1",
    "tape": "^4.6.3",
    "xvfb-maybe": "^0.2.1"
  },
  "homepage": "https://github.com/hypermodules/hyperamp",
  "keywords": [
    "amp",
    "hyper",
    "music",
    "player"
  ],
  "license": "ISC",
  "main": "main/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypermodules/hyperamp.git"
  },
  "scripts": {
    "start": "electron main",
    "test": "run-s test:*",
    "test:deps": "dependency-check ./package.json --entry renderer/index.js --no-dev --ignore-module electron",
    "test:lint": "standard | snazzy",
    "test:tape": "xvfb-maybe tape test/* | tap-format-spec",
    "rebuild": "electron-rebuild",
    "build": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.hypermodules.hyperamp",
    "mac": {
      "category": "public.app-category.music",
      "target": [
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
